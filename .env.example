# Environment variables for NUDEX Playback Service

# === SERVER ===
GO_ENV=development
PORT=8085

# === REDIS ===
REDIS_URL=redis://nudex_redis_pass@redis:6379

# === RABBITMQ ===
RABBITMQ_URL=amqp://nudex_rabbit:nudex_rabbit_pass@rabbitmq:5672/nudex

# === PLAYBACK TOKENS ===
TOKEN_SECRET=nudex_playback_token_secret_dev
TOKEN_TTL=3600

# === RATE LIMITING ===
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=60

# === LOGGING ===
LOG_LEVEL=info